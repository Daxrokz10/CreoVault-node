    <section class="login-form" id="login-form">
        <div class="container">
          <div class="row bg align-items-center">
            <div class="col-6">
              <div class="img my-4"></div>
            </div>
            <div class="col-6">
              <div class="content">
                <div class="logo d-flex justify-content-center pt-3">
                  <img src="./Homepage/images/nav-logo.png" alt="" />
                </div>
                <div class="text text-center pt-5 pb-3">
                  <h3>Hi Designer</h3>
                  <p>Welcome to CreoVault</p>
                </div>

                <form action="" class="d-flex justify-content-center flex-wrap">
                  <input type="text" class="username" placeholder="Username" />
                  <input
                    type="password"
                    class="password"
                    placeholder="Password"
                  />
                </form>
                <div class="other-option d-flex justify-content-around pt-3">
                  <span>Don’t have Account</span>
                  <span>Forgot Password?</span>
                </div>
                <div class="btn d-flex justify-content-center mt-5">
                  <button class="animated-button" id="login-btn">E N T R Y</button>
                  <button class="animated-button ms-3" id="guest-btn">Login as Guest</button>
                </div>
                <div class="icon text-center pt-3">
                    <h4>Or Login with</h4>
                    <i class="fa-brands fa-google"></i>
                    <i class="fa-brands fa-github"></i>
                    <i class="fa-brands fa-linkedin"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>

    <main id="main-content" style="display:none;">
      <!-- hero section -->
      <section class="hero-section mb-5">
        <div class="container">
          <div class="row">
            <div class="col-12 col-md-7">
              <div class="hero-text h-100" style="margin-top: 100px">
                <h1 class="hero-title text-black">
                  Creative Projects, One Click Away.
                  <a href="#" class="text-black">
                    <span
                      class="arrow-icon border border-1 border-black rounded-circle"
                    >
                      <i class="fa-solid fa-arrow-right"></i>
                    </span>
                  </a>
                </h1>
                <p class="mt-3">
                  Explore a curated marketplace where designers and developers
                  showcase their best work, ready to purchase, download, or bid
                  on instantly. Whether you're looking for a sleek UI kit, a
                  powerful website template, or a one-of-a-kind digital asset,
                  it's all here. No hassle. Just click and create.
                </p>
              </div>
            </div>
            <div class="col-12 col-md-5">
              <div class="hero-img position-relative mt-3 ms-5">
                <img
                  src="./Homepage/images/banner-1.png"
                  alt=""
                  class="position-absolute img-2"
                />
                <img
                  src="./Homepage/images/banner-2.png"
                  alt=""
                  class="position-absolute img-3"
                />
                <img
                  src="./Homepage/images/banner-3.png"
                  alt=""
                  class="position-absolute img-1"
                />
              </div>
            </div>
            <div class="col-12 col-md-8">
              <div class="info rounded-4 d-flex mt-2 flex-wrap">
                <div class="col-12 col-md-4">
                  <div class="info-img d-flex px-5 py-3">
                    <img
                      src="./Homepage/images/info-logo.png"
                      alt=""
                      class="bg-white p-4 rounded-4 mt-4"
                    />
                  </div>
                </div>
                <div class="col-12 col-md-8">
                  <p class="text-white py-3">
                    Talented designers and skilled developers bring their vision
                    to life on our platform. From stunning visuals to powerful
                    code, every project is crafted with creativity, precision,
                    and passion ready to elevate your next big idea.
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
